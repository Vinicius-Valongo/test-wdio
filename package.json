{
  "name": "wdio-native-demo-automation",
  "version": "1.0.0",
  "type": "commonjs",
  "description": "WebDriverIO Native Demo App Automation",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_OPTIONS= wdio run wdio.conf.js",
    "test:android": "cross-env NODE_OPTIONS= wdio run wdio.conf.js --spec ./test/specs/**/*.spec.js",
    "test:ios": "cross-env NODE_OPTIONS= wdio run wdio.conf.js --spec ./test/specs/**/*.spec.js",
    "report:generate": "allure generate allure-results --clean -o allure-report",
    "report:open": "allure open allure-report",
    "report:serve": "allure serve allure-results",
    "test:report": "npm test && npm run report:generate && npm run report:open",
    "clean:reports": "node scripts/clean-reports.js"
  },
  "keywords": [
    "webdriverio",
    "appium",
    "mobile",
    "automation"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@wdio/allure-reporter": "^9.20.0",
    "@wdio/appium-service": "^9.20.0",
    "@wdio/cli": "^9.20.0",
    "@wdio/local-runner": "^9.20.0",
    "@wdio/mocha-framework": "^9.20.0",
    "@wdio/spec-reporter": "^9.20.0",
    "allure-commandline": "^2.25.0",
    "appium": "^2.0.0",
    "appium-uiautomator2-driver": "^2.0.0",
    "appium-xcuitest-driver": "^5.0.0",
    "cross-env": "^10.1.0",
    "fs-extra": "^11.1.1"
  },
  "dependencies": {
    "chai": "^4.3.10"
  }
}
